class No:
    def __init__(self, valor):
        self.valor = valor
        self.proximo = None
    
    def mostrarNo(self):
        print(self.valor)


class ListaEncadeada:
    def __init__(self):
        self.primeiro = None

    def mostrar(self):
        if self.primeiro is None:
            print("Erro de lista vazia")
            return
        atual = self.primeiro
        while atual is not None:
            atual.mostrarNo()
            atual = atual.proximo

    def inserirInicio(self, valor):
        novo = No(valor)
        novo.proximo = self.primeiro
        self.primeiro = novo

    def excluirInicio(self):
        if self.primeiro is None:
            print("Erro de lista vazia")
            return None
        temp = self.primeiro
        self.primeiro = self.primeiro.proximo
        return temp


lista = ListaEncadeada()
nome = "Jean"
for letra in nome[::-1]:  # Inserindo de tr√°s para frente para manter a ordem correta
    lista.inserirInicio(letra)
